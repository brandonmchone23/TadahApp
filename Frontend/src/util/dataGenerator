function generateRandomId() {
    return Math.floor(Math.random() * 1000); // Generate a random ID
}

function generateRandomUsername() {
    const usernames = ['Alice', 'Bob', 'Charlie', 'David', 'Eve']; // List of possible usernames
    const randomIndex = Math.floor(Math.random() * usernames.length);
    return usernames[randomIndex]; // Pick a random username
}

function generateRandomTaskTitle() {
    const tasks = ['Task 1', 'Task 2', 'Task 3', 'Task 4', 'Task 5']; // List of possible task titles
    const randomIndex = Math.floor(Math.random() * tasks.length);
    return tasks[randomIndex]; // Pick a random task title
}

function generateRandomParent() {
    const parent = {
        id: generateRandomId(),
        username: "brandon",
        children: []
    };

    for (let i = 0; i < 2; i++) {
        const child = generateRandomChild();
        parent.children.push(child);
    }

    return parent;
}

function generateRandomChild() {
    const child = {
        childId: generateRandomId(),
        username: generateRandomUsername(),
        task: generateRandomTask()
    };

    return child;
}

function generateRandomTask() {
    const task = {
        taskId: generateRandomId(),
        title: generateRandomTaskTitle(),
        isCompleted: Math.random() < 0.5 // Generate a random boolean value for isCompleted
    };

    return task;
}

// Generate a random parent
const parent = generateRandomParent();

console.log(parent);
